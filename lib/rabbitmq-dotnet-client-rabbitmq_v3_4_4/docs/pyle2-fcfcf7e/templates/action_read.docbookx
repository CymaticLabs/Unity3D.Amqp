## -*- nxml -*-
<?xml version="1.0"?>
<!DOCTYPE article PUBLIC "-//OASIS//DTD DocBook XML V4.3//EN"
                         "http://www.oasis-open.org/docbook/xml/4.3b2/docbookx.dtd">
<article>
  <title>$escape($page.title)</title>
  #if $page.version
  <articleinfo>
    <revhistory>
      <revision>
	<revnumber>$page.friendly_version</revnumber>
	<date>$escape($page.timestamp)</date>
      </revision>
    </revhistory>
  </articleinfo>
  #end if
  $page.render('docbookx')
</article>
